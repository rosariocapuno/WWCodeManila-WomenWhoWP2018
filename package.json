{
  "name": "wwcodemanila-womenwhowp2018",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.6",
    "@fortawesome/free-solid-svg-icons": "^5.4.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "node-sass-chokidar": "^1.3.4",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-bootstrap-validation": "^0.1.11",
    "react-dom": "^16.5.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.4",
    "reactstrap": "^6.5.0",
    "sass-loader": "^7.1.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/assets/styles/scss -o src/assets/styles/css",
    "watch-css": "yarn build-css && node-sass-chokidar src/assets/styles/scss -o src/assets/styles/css --watch --recursive",
    "dev": "concurrently \"yarn watch-css\" \"yarn start\"",
    "start": "react-scripts start",
    "build": "cross-env NODE_ENV=production webpack --config build/webpack.config.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0"
  }
}
